{
  "name": "@elizaos-plugins/adapter-postgres",
  "version": "0.1.9",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "schema.sql",
    "seed.sql"
  ],
  "dependencies": {
    "@elizaos/core": "0.25.9",
    "@types/pg": "8.11.10",
    "pg": "8.13.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "tsup": "8.3.5",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsup --format esm,cjs --dts",
    "dev": "tsup --format esm,cjs --dts --watch"
  }
}